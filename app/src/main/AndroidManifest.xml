<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bozonpee.canvasaccelerometer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Jumping Jack"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Game" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.GAME" />
            </intent-filter>
        </activity>
        <activity android:name=".Result" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.RESULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>